-- GodzHub Home/Start

local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Godz Hub",
   Icon = "menu",
   LoadingTitle = "Godz Hub | Loading...",
   LoadingSubtitle = "",
   Theme = "Ocean",

   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false,

   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "Godz | AutoSave"
   },

   Discord = {
      Enabled = true,
      Invite = "https://discord.gg/Z7kzY6YNKH",
      RememberJoins = true
   },

   KeySystem = true,
   KeySettings = {
      Title = "Godz Hub | Key System",
      Subtitle = "",
      Note = "Key site: https://loot-link.com/s?UjRmX7Xq",
      FileName = "Key | Godz Hub",
      SaveKey = true,
      GrabKeyFromSite = true,
      Key = "https://pastebin.com/raw/BF5fkdXV"
   }
})

Rayfield:Notify({
   Title = "GodzHub | Notification",
   Content = "Script executado e key valida!",
   Duration = 3.0,
   Image = "check",
})

